<%= render partial: './shared/header' %>

<p><%= button_to "Create a New User", register_path, method: :get %></p>

<div id="users">
  <h2>Existing Users</h2>
  <% @users.each do |user| %>
    <div id="user_<%= user.id %>">
      <li><%= link_to user.email, user_path(user) %></li>
    </div>  
  <% end %> 
</div>
